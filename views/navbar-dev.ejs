                  .viewAll .maage-nav-link::after {
                    content: '';
                    display: inline-block;
                    width: 16px;
                    height: 16px;
                    position: absolute;
                    right: 10px;
                    top: 50%;
                    transform: translateY(-50%);
                    background-repeat: no-repeat;
                    opacity: 0;
                    transition: opacity 0.2s ease-in-out, transform 0.2s ease-in-out;
